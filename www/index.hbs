<!doctype html>
<html lang="en">
<head>
	<base href="/">
  <meta charset="UTF-8">
  <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
	<meta property="og:site_name" content="{{siteName}}"/>
	<meta property="og:title" content="{{ogTitle}}"/>
	<meta property="og:image" content="{{ogImage}}"/>
	<meta property="og:description" content="{{ogDescription}}"/>
	<meta name="adx:sections" content=""/>

	<title>{{siteName}}</title>
  <link rel="SHORTCUT ICON" href="{{siteFav}}">
  <link rel="stylesheet" href="material-icon/css/googlematerialdesign.css"/>
  <link rel="stylesheet" href="lib/ngprogress/ngProgress.css"/>
  <link rel="stylesheet" href="lib/angular-spinkit/build/angular-spinkit.min.css"/>
  <link rel="stylesheet" href="bundle.css"/>

  <script src="lib/jquery/dist/jquery.min.js"></script>
  <script src="lib/angular/angular.min.js"></script>
  <script src="lib/angular-ui-router/release/angular-ui-router.min.js"></script>
  <script src="lib/angular-touch/angular-touch.min.js"></script>

</head>
<body ng-controller="appCtrl as appCtrl">
  <div ui-view="layout" id="root" ng-class="{ready: appCtrl.ready}"></div>

  <script src="lib/underscore/underscore-min.js"></script>
  <script src="lib/gsap/src/minified/TweenMax.min.js"></script>
  <script src="lib/gsap/src/minified/easing/EasePack.min.js"></script>
  <script src="lib/gsap/src/minified/plugins/ScrollToPlugin.min.js"></script>
  <script src="lib/fastclick/lib/fastclick.js"></script>
  <script src="lib/accounting/accounting.min.js"></script>
  <script src="lib/angular-animate/angular-animate.min.js"></script>
  <script src="lib/ngprogress/build/ngprogress.min.js"></script>
  <script src="lib/angular-spinkit/build/angular-spinkit.min.js"></script>
  <script src="lib/ng-simple-parallax/js/ngParallax.min.js"></script>
  <script src="lib/moment/min/moment.min.js"></script>
  <script src="bundle.js"></script>

	{{#if googleAnalyticId}}
		<!--Google Analytics!-->
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
						(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
					m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

			ga('create', '{{googleAnalyticId}}', 'auto');
			ga('send', 'pageview');
		</script>
	{{/if}}




  <!--Facebook scripts-->


	{{#if facebookPixelId}}
		<script>
			//Pixel code!
      !function(f,b,e,v,n,t,s){if(f.fbq)return;n=f.fbq=function(){n.callMethod?
					n.callMethod.apply(n,arguments):n.queue.push(arguments)};if(!f._fbq)f._fbq=n;
				n.push=n;n.loaded=!0;n.version='2.0';n.queue=[];t=b.createElement(e);t.async=!0;
				t.src=v;s=b.getElementsByTagName(e)[0];s.parentNode.insertBefore(t,s)}(window,
					document,'script','https://connect.facebook.net/en_US/fbevents.js');

			fbq('init', '{{facebookPixelId}}');

		</script>
        <noscript>
            <img height="1" width="1" style="display:none" src="https://www.facebook.com/tr?id={{facebookPixelId}}&ev=PageView&noscript=1">
        </noscript>
	{{/if}}

</body>
</html>